{
  "name": "cloudtay/ripple",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Ripple\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "cclilshy",
      "email": "jingnigg@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-posix": "*",
    "ext-pcntl": "*",
    "ext-sockets": "*",
    "ext-openssl": "*",
    "ext-ffi": "*",
    "cloudtay/ripple-kernel": "^1.0",
    "cloudtay/ripple-database": "^1.0"
  },
  "require-dev": {
    "ext-curl": "*",
    "ext-parallel": "*",
    "ext-zlib": "*",
    "phpunit/phpunit": "*",
    "friendsofphp/php-cs-fixer": "^3.68"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
